+ f *.mov | f *.MOV | f *.avi | f *.AVI | f *.mp4 | f *.MP4 & tr
c cut video [no screen]
  #! /usr/bin/bash
  clear
  read -e -p "Start : " -i "00:00:00" start
  read -e -p "Stop  : " -i "00:00:00" stop
  echo "Please Wait..."
  mpv -ss $start -endpos $stop -really-quiet -ovc copy -oac pcm %f -o %b-CUT.%x
  ~/scripts/iftt-maker "Rasperry Pi" "La vidéos %d.avi est coupée"

+ f *.jpg | f *.JPG & t t
s Autolevel photos
  set %t
  while [ -n "$1" ]; do
    mogrify -auto-level $1
    shift
  done
  
+ f *.tiff | f *.TIFF & t t
s Tiff to jpg with sharp
  set %t
  while [ -n "$1" ]; do
    sharp --quality 100 -o $1-sharp.jpg $1
    shift
  done

+ f *.jpg | f *.JPG & t r
e Affiche l'EXIF de la photo
  clear
  exiv2 -pv %f
  printf 'Press [Enter] key to continue...'
  read null

+ f *.jpg | f *.JPG & t r
m Modifie la date EXIF de la photo
  if [ -f /tmp/.photos.sh ] ; then whiptail --title "Photos" --msgbox "Busy try Later..." 12 35; exit; fi
  cat > /tmp/.photos.sh << 'EOF'
  clear
  EXIF=`exiv2 -q "%f" 2> null | grep "Image timestamp" | cut -d ' ' -f 4,5`
  printf "\033[00m%f\n\033[01m"
  read -e -p "Date : " -i "$EXIF" CHANGE
  exiv2 -q -M "add Exif.Photo.DateTimeOriginal $CHANGE" "%f"
  rm /tmp/.photos.sh
  EOF
  bash /tmp/.photos.sh
  
+ f *.pdf | f *.PDF & t t
p Transforme un fichier PDF sécurisé en normal
  set %t
  while [ -n "$1" ]; do
    gs -dBATCH -dNOPAUSE -q -sDEVICE=pdfwrite -sOutputFile="$1.test.pdf" "$1"
    shift
  done
