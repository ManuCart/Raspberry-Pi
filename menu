+ f *
g git push
  git add %f
  PARAM=%{parameters}
  git commit -m "$PARAM"
  git push

+ f *
h git pull
  git pull

+ f * & t t
r       run
        PARAM=%{parameters}
        %f $PARAM

= d *
C castnow 
  echo %d/
  castnow %d/ --type "audio/wav"
  
+ f * & t t
D Delete
  set %t
  while [ -n "$1" ] ; do
    rm -rf "$1"
    shift
  done

= d *
C Copy [X]
  tmux send -t X 'rsync -a --progress --info=progress2 "%D/" "%d"' ENTER
  tmux select-window -t pi:X
  tmux -2 attach -t pi

= d *
C Move [X]
  tmux send -t X 'rsync -a --progress --remove-sent-files --info=progress2 "%D/" "%d"' ENTER
  tmux select-window -t pi:X
  tmux -2 attach -t pi

+ f * & t t
B move to "Freebox Server" [X]
  if [ -f /tmp/.freebox.sh ] ; then whiptail --title "Move to Freebox" --msgbox "Busy try Later..." 12 35; exit; fi
  cat > /tmp/.freebox.sh << 'EOF'
  clear
  sudo mount -t cifs //mafreebox.freebox.fr/Server\ Freebox/  /media/freebox -o user=freebox,password=Freebox4K,uid=1000,gid=1000,rw,sec=ntlm
  cd %d/
  set %t
  while [ -n "$1" ]; do
    echo "Copy : $1"
    rsync --archive --no-o --no-g --progress --remove-sent-files "$1" "/media/freebox"
    shift
  done
  ~/scripts/iftt-maker "Raspberry Pi" "Files moved to Freebox Server"
  rm /tmp/.freebox.sh
  EOF
  tmux has-session -t pi ; if [ "$?" -eq 1 ] ; then ~/scripts/tmux ; fi
  tmux send -t X 'sh /tmp/.freebox.sh' ENTER
  tmux select-window -t pi:X
  tmux -2 attach -t pi

+ f * & t t
B upload to dropbox [Y]
  if [ -f /tmp/.dropbox.sh ] ; then whiptail --title "Upload to Dropbox" --msgbox "Busy try Later..." 12 35; exit; fi
  cat > /tmp/.dropbox.sh << 'EOF'
  clear
  cd "%d"
  for f in %t; do
    dropbox_uploader upload "$f" "$f"
  done
  ~/scripts/iftt-maker "Raspberry Pi" "Files Uploaded to Dropbox"
  rm /tmp/.dropbox.sh
  EOF
  tmux has-session -t pi ; if [ "$?" -eq 1 ] ; then ~/scripts/tmux ; fi
  tmux send -t Y 'sh /tmp/.dropbox.sh' ENTER
  tmux select-window -t pi:Y
  tmux -2 attach -t pi

+ f * & t r
C download from dropbox
  dropbox_uploader download %{Dropbox Filename}

+ f * & t t
D upload to google drive [Y]
  if [ -f /tmp/.gdrive.sh ] ; then whiptail --title "Upload to Google Drivex" --msgbox "Busy try Later..." 12 35; exit; fi
  cat > /tmp/.gdrive.sh << 'EOF'
  clear
  rm -rf /media/hdd/Drive/Upload/*
  mkdir /media/hdd/Drive/Upload
  cd "%d"
  for f in %t; do
    cp "$f" /media/hdd/Drive/Upload
  done
  cd /media/hdd/Drive
  drive push --quiet Upload/*
  ~/scripts/iftt-maker "Raspberry Pi" "Files Uploaded to Google Drive"
  rm /tmp/.gdrive.sh
  EOF
  tmux has-session -t pi ; if [ "$?" -eq 1 ] ; then ~/scripts/tmux ; fi
  tmux send -t Y 'sh /tmp/.gdrive.sh' ENTER
  tmux select-window -t pi:Y
  tmux -2 attach -t pi

+ f * & t r
A Tmux Session A
  tmux send -t A 'cd ~/scripts && clear' ENTER
  tmux send -t A 'bash %f' ENTER
  tmux select-window -t pi:A
  tmux -2 attach -t pi
  
