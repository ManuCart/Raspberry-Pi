# Loop through directories and change creating date
  FILES=/media/hdd/photos
  find $FILES -type d | while read DIR ; do
  NAME=`echo "$DIR" | rev | cut -d '/' -f 1 | rev | cut -c12-`
  echo "$DIR --->>> /media/hdd/photos/$NAME"
  echo "$NAME" >> result.txt
  done
  sort result.txt | uniq --count | sort
